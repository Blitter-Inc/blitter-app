{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": [
				"./src/assets/*"
			],
			"@components/*": [
				"./src/app/components/*"
			],
			"@navigations/*": [
				"./src/app/navigations/*"
			],
			"@screens/*": [
				"./src/app/screens/*"
			],
			"@store/*": [
				"./src/app/store/*"
			],
		}
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}
