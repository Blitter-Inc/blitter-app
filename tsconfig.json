{
  "compilerOptions": {
    // "strict": true,
    "allowJs": false,
    "typeRoots": ["./src/types"],
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@config/*": [
        "src/app/config/*"
      ],
      "@navigations/*": [
        "src/app/navigations/*"
      ],
      "@screens/*": [
        "src/app/screens/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@store/*": [
        "src/app/store/*"
      ],
    },
  },
  "extends": "expo/tsconfig.base"
}