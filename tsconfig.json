{
  "compilerOptions": {
    "strict": true,
    "allowJs": false,
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "$assets/*": [
        "src/assets/*"
      ],
      "$components/*": [
        "src/app/components/*"
      ],
      "$config/*": [
        "src/app/config/*"
      ],
      "$modules/*": [
        "src/app/modules/*"
      ],
      "$navigations/*": [
        "src/app/navigations/*"
      ],
      "$services/*": [
        "src/app/services/*"
      ],
      "$store/*": [
        "src/app/store/*"
      ],
      "$types/*": [
        "src/app/types/*"
      ],
    },
  },
  "extends": "expo/tsconfig.base"
}